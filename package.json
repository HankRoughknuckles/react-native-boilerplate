{
  "name": "reactNativeBoilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node_modules/.bin/react-native start --transformer node_modules/react-native-typescript-transformer/index.js --sourceExts ts,tsx",
    "test": "jest"
  },
  "dependencies": {
    "@types/jasmine": "^2.8.6",
    "@types/react": "^16.0.40",
    "@types/react-native": "^0.52.18",
    "@types/react-redux": "^5.0.15",
    "@types/react-test-renderer": "^16.0.1",
    "appcenter": "^1.4.0",
    "appcenter-analytics": "^1.4.0",
    "appcenter-crashes": "^1.4.0",
    "react": "^16.3.0-alpha.1",
    "react-native": "0.54.2",
    "react-native-code-push": "^5.3.2",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "tslib": "^1.9.0",
    "typescript": "^2.7.2"
  },
  "devDependencies": {
    "@types/jest": "^22.2.0",
    "babel-jest": "22.4.3",
    "babel-preset-react-native": "4.0.0",
    "jest": "22.4.2",
    "react-native-cli": "^2.0.1",
    "react-native-typescript-transformer": "^1.2.3",
    "react-test-renderer": "^16.3.0-alpha.1",
    "ts-jest": "^22.4.2"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
